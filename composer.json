   "require": {
        "composer/installers": "^2.0.2",
        "wpackagist-theme/storefront": "^4.6.0",
        "wpackagist-plugin/woocommerce": "^9.0.2",
        "wpackagist-theme/boutique": "^2.0",
        "wpackagist-plugin/live-auto-refresh": "^1.0"
